<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 当前窗口失去焦点时 -->
    <item android:drawable="@drawable/ic_dataconnect_off" android:state_window_focused="false" />
    <!-- 不可用时 -->
    <item android:drawable="@drawable/ic_qs_back" android:state_enabled="false" />
    <!-- 按压时 -->
    <item android:drawable="@drawable/ic_qs_airplane_on" android:state_pressed="true" />
    <!-- 被选中时 -->
    <item android:drawable="@drawable/ic_qs_flashlight_on" android:state_selected="true" />
    <!-- 被激活时 -->
    <item android:drawable="@drawable/ic_qs_rotation_portrait" android:state_activated="true" />
    <!-- 默认时 -->
    <item android:drawable="@drawable/number1_g" />
</selector>